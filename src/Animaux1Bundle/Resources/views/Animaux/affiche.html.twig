<h1>Liste des Animaux </h1>
<table border="1">
    <tr>
        <th>Nom</th>
        <th>Race</th>
        <th>Date de naissance</th>
        <th>Espece</th>
        <th>Sexe</th>
        <th>Modifier</th>
        <th>Supprimer</th>

    </tr>

    {% for voitures in m %}
    <tr>
        <td> {{ voitures.nom }}</td>
        <td> {{ voitures.race }}</td>
        <td> {{ voitures.dateNaissance|date('Y-m-d') }}</td>
        <td> {{ voitures.espece }}</td>
        <td> {{ voitures.sexe }}</td>
        <td> <a href="{{ path ('modifier',{'id':voitures.id}) }}">Modifier</a></td>
        <td> <a href="{{ path ('supprimer',{'id':voitures.id}) }}">Supprimer</a></td>


    </tr>
{% endfor %}